option solver gurobi;

var x {p in 1 .. 8} >= 0; # quantidade de pratos i consumidos

minimize p: 319*x[1] + 259*x[2] + 229*x[3] + 289*x[4] + 189*x[5] + 199*x[6] + 199*x[7] + 249*x[8];

s.t. R1: 60*x[1] +  8*x[2] +  8*x[3] + 40*x[4] + 15*x[5] + 70*x[6] + 25*x[7] + 60*x[8] >= 700;
s.t. R2: 20*x[1] +  0*x[2] + 10*x[3] + 40*x[4] + 35*x[5] + 30*x[6] + 50*x[7] + 20*x[8] >= 700;
s.t. R3: 10*x[1] + 20*x[2] + 15*x[3] + 35*x[4] + 15*x[5] + 15*x[6] + 25*x[7] + 15*x[8] >= 700;
s.t. R4: 15*x[1] + 20*x[2] + 10*x[3] + 10*x[4] + 15*x[5] + 15*x[6] + 15*x[7] + 10*x[8] >= 700;

# p* = 8820